    rdfs:isDefinedBy "https://en.wikipedia.org/wiki/Hepatitis_E"^^xsd:string,
        "https://nijz.si/nalezljive-bolezni/nalezljive-bolezni-od-a-do-z/hepatitis-e-virusni-hepatitis-e/"^^xsd:string,
