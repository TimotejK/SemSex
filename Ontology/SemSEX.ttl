    rdfs:subClassOf <http://onto.clarin.si/semsex#vedenjska_»naravna«> ;
    owl:sameAs <http://wikidata.org/entity/Q182207> ;
    skos:altLabel "1.2.5.2"@sl ;
    skos:definition "Coitus Interruptus: A method of birth control involving the withdrawal of the penis from the vagina before ejaculation to prevent insemination. It does not protect against sexually transmitted infections (STIs)."@en,
        "Coitus Interruptus: manj zanesljiva metoda kontracepcije, ki vključuje umik penisa iz nožnice pred ejakulacijo, da se prepreči oploditev. Ne ščiti pred spolno prenosljivimi okužbami (SPO)."@sl .

ss:diafragma a owl:Class ;
    rdfs:label "Diaphragm (birth control)"@en,
        "diafragma"@sl ;
    ss:povezano_z ss:maternicni_vrat ;
    rdfs:isDefinedBy "https://en.wikipedia.org/wiki/Diaphragm_(birth_control)"^^xsd:string ;
    rdfs:subClassOf ss:pregradna ;
    owl:sameAs <http://wikidata.org/entity/Q754926> ;
    skos:altLabel "1.2.1.2"@sl ;
    skos:definition "Diaphragm (Birth Control): A barrier method of birth control placed over the cervix with spermicide before sex to block access to the uterus and hold spermicide near the cervix. It has moderate effectiveness in preventing pregnancy."@en,
        "Diafragma (kontracepcija): pregradna metoda kontracepcije, ki se pred spolnim odnosom namesti na maternični vrat s spermicidom, da prepreči dostop do maternice in zadrži spermicid blizu materničnega vratu. Ima zmerno učinkovitost pri preprečevanju nosečnosti."@sl .

ss:dolocitev_bioloskega_spola a owl:Class ;
    rdfs:label "Sex-determination system"@en,
        "Določitev biološkega spola"@sl ;
    schemaorg:isVariantOf ss:bioloski_spol ;
    rdfs:isDefinedBy "https://en.wikipedia.org/wiki/Sex-determination_system"^^xsd:string ;
    rdfs:subClassOf ss:oploditev ;
    owl:sameAs <http://wikidata.org/entity/Q594595> ;
    skos:altLabel "6.1.1"@sl ;
    skos:definition "Sex-Determination System: A biological system determining the development of sexual characteristics in organisms, such as male and female sexes, hermaphroditism, or sex determination by environmental factors or genetics."@en,
        "Sistem določanja spola: biološki sistem, ki določa razvoj spolnih značilnosti v organizmih, kot sta moški in ženski spol, hermafroditizem ali določanje spola z okoljskimi dejavniki ali genetiko."@sl .


ss:drag a owl:Class ;
    rdfs:label "Drag (entertainment)"@en,
        "Drag"@sl ;
    rdfs:isDefinedBy "https://en.wikipedia.org/wiki/Drag_(entertainment)"^^xsd:string ;
    rdfs:subClassOf ss:spolni_izraz ;
    owl:sameAs <http://wikidata.org/entity/Q3410369> ;
    skos:altLabel "3.1.3"@sl ;
    skos:definition "Drag: A performance of exaggerated gender expression, where individuals, often in theatrical settings, portray either exaggerated femininity or masculinity. Drag is often used for entertainment, social satire, and political commentary."@en,
        "Drag: Predstava s pretiranim izražanjem spola, kjer posamezniki, pogosto v gledaliških okoljih, prikazujejo pretirano ženskost ali moškost. Drag se pogosto uporablja za zabavo, družbeno satiro in politične komentarje."@sl .

ss:druzbeni_spol a owl:Class ;
    rdfs:label "Gendre"@en,
        "Družbeni spol"@sl ;
    rdfs:isDefinedBy "https://en.wikipedia.org/wiki/Gender"^^xsd:string,
        "https://sl.wikipedia.org/wiki/Družbeni_spol"^^xsd:string ;
    rdfs:subClassOf ss:spolna_identiteta ;
    owl:sameAs <http://wikidata.org/entity/Q48277> ;
    skos:altLabel "2.1.1.2"@sl ;
    skos:definition "Gendre: Gender includes the social, psychological, cultural and behavioral aspects of being a man, woman, or other gender identity. Depending on the context, this may include sex-based social structures (i.e. gender roles) and gender expression."@en,
        "Družbeni spol: Družbeni spol je nabor značilnosti, ki se nanašajo na moškost in ženskost in ki ju razlikujejo med seboj. Glede na kontekst lahko te značilnosti vključujejo biološki spol (moški, ženska ali medspol), spolne socialne strukture (spolne in druge družbene vloge) ter spolno identiteto."@sl .

ss:erektilna_disfunkcija a owl:Class ;
    rdfs:label "Erectile dysfunction"@en,
        "Erektilna disfunkcija"@sl ;
    ss:povezano_z ss:penis ;
