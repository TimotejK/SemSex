ss:kombinirana_tabletka a owl:Class ;
    rdfs:label "Combined oral contraceptive pill"@en,
        "kombinirana tabletka"@sl ;
    rdfs:isDefinedBy "https://en.wikipedia.org/wiki/Combined_oral_contraceptive_pill"^^xsd:string ;
