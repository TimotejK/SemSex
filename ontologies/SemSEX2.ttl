ss:poliginija a owl:Class ;
    rdfs:label "Polygamy"@en,
        "Poliginija"@sl ;
    rdfs:isDefinedBy "https://en.wikipedia.org/wiki/Polygyny"^^xsd:string ;
    rdfs:subClassOf ss:promiskuitetno_vedenje ;
