ss:mehurcasta_zleza a owl:Class ;
    rdfs:label "Bulbourethral gland"@en,
        "Mehurčasta žleza"@sl ;
    rdfs:isDefinedBy "https://en.wikipedia.org/wiki/Bulbourethral_gland"^^xsd:string,
