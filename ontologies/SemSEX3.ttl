    rdfs:subClassOf ss:hormonska ;
    owl:sameAs <http://wikidata.org/entity/Q55614474> ;
    skos:altLabel "1.2.2.4"@sl ;
    skos:definition "Combined Injectable Contraceptives (CICs): Combined injectable contraceptives are monthly injections containing both estrogen and progestin hormones to prevent pregnancy. They work by inhibiting ovulation and thickening cervical mucus."@en,
        "Kombinirani kontraceptivi za injiciranje (CIC): Kombinirani kontraceptivi za injiciranje so mesečne injekcije, ki vsebujejo hormone estrogen in progestin za preprečevanje nosečnosti. Delujejo tako, da zavirajo ovulacijo in zgostijo sluz materničnega vratu."@sl .

ss:kontracepcijski_implantat a owl:Class ;
    rdfs:label "Contraceptive implant"@en,
        "Kontracepcijski implantat"@sl ;
    rdfs:isDefinedBy "http://www.cbz.si/zzzs/pao/bazazdr2.nsf/o/5313E92FCF976A89C1257B4B00836C4C/$File/a-016142.pdf"^^xsd:string,
